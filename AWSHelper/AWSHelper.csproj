<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <TargetLatestRuntimePatch>true</TargetLatestRuntimePatch>
    <Product>AWSHelper</Product>
    <Description>Amazon Web Services .NET Standard Extentions</Description>
    <PackageId>Asmodat.AWSHelper</PackageId>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageReleaseNotes>first release</PackageReleaseNotes>
    <Version>1.0.0</Version>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <Company>Asmodat</Company>
    <Authors>Asmodat</Authors>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="install.sh" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.AutoScaling" Version="3.3.6.1" />
    <PackageReference Include="AWSSDK.AutoScalingPlans" Version="3.3.1.12" />
    <PackageReference Include="AWSSDK.CloudFormation" Version="3.3.11.11" />
    <PackageReference Include="AWSSDK.CloudWatch" Version="3.3.6.13" />
    <PackageReference Include="AWSSDK.CloudWatchLogs" Version="3.3.6" />
    <PackageReference Include="AWSSDK.Core" Version="3.3.25.2" />
    <PackageReference Include="AWSSDK.S3" Version="3.3.23.1" />
    <PackageReference Include="AWSSDK.EC2" Version="3.3.59.2" />
    <PackageReference Include="AWSSDK.ECR" Version="3.3.3.19" />
    <PackageReference Include="AWSSDK.ECS" Version="3.3.17.4" />
    <PackageReference Include="AWSSDK.ElasticLoadBalancing" Version="3.3.3.1" />
    <PackageReference Include="AWSSDK.ElasticLoadBalancingV2" Version="3.3.10.6" />
    <PackageReference Include="AWSSDK.IdentityManagement" Version="3.3.7.7" />
    <PackageReference Include="AWSSDK.ResourceGroups" Version="3.3.1.5" />
    <PackageReference Include="AWSSDK.Route53" Version="3.3.16.1" />
    <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.3.1.10" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\AWSWrapper\AWSWrapper.csproj" />
  </ItemGroup>

</Project>
