<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <TargetLatestRuntimePatch>true</TargetLatestRuntimePatch>
    <Product>AWSHelper</Product>
    <Description>Amazon Web Services .NET Standard Extentions</Description>
    <PackageId>Asmodat.AWSHelper</PackageId>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageReleaseNotes>first release</PackageReleaseNotes>
    <Version>1.0.0</Version>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <Company>Asmodat</Company>
    <Authors>Asmodat</Authors>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="install.sh" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.AutoScaling" Version="3.3.5.1" />
    <PackageReference Include="AWSSDK.AutoScalingPlans" Version="3.3.1.10" />
    <PackageReference Include="AWSSDK.CloudFormation" Version="3.3.11.9" />
    <PackageReference Include="AWSSDK.CloudWatch" Version="3.3.6.11" />
    <PackageReference Include="AWSSDK.CloudWatchLogs" Version="3.3.5.2" />
    <PackageReference Include="AWSSDK.Core" Version="3.3.25" />
    <PackageReference Include="AWSSDK.S3" Version="3.3.21" />
    <PackageReference Include="AWSSDK.EC2" Version="3.3.59" />
    <PackageReference Include="AWSSDK.ECR" Version="3.3.3.17" />
    <PackageReference Include="AWSSDK.ECS" Version="3.3.17.2" />
    <PackageReference Include="AWSSDK.ElasticLoadBalancing" Version="3.3.2.14" />
    <PackageReference Include="AWSSDK.ElasticLoadBalancingV2" Version="3.3.10.4" />
    <PackageReference Include="AWSSDK.IdentityManagement" Version="3.3.7.5" />
    <PackageReference Include="AWSSDK.ResourceGroups" Version="3.3.1.3" />
    <PackageReference Include="AWSSDK.Route53" Version="3.3.15.14" />
    <PackageReference Include="AWSSDK.SimpleNotificationService" Version="3.3.1.8" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\AWSWrapper\AWSWrapper.csproj" />
  </ItemGroup>

</Project>
